<div class="panel panel-primary col-md-4 movie-card">
    <div class="panel-heading">
        <h3>{{title}}</h3>
    </div>
<div class="panel-body">
<h4>{{year}}</h4>
<img class="movie-card-image" src="{{posterURL}}">
  </div>
  <div class="panel-footer">
  <button type="button" class="deleteButton">Delete This Movie</button>
  <h4>{{watched}}</h4>
  <h4 class="hidden">Your Rating: {{rating}} </h4>
  <select class="hidden" id="ratingMenu">
  <option value="1">1</option>
  <option value="2">2</option>
  <option value="3">3</option>
  <option value="4">4</option>
  <option value="5">5</option>
</select>
  </div>
</div>



