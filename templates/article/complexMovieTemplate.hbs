<div class="panel panel-primary col-md-4 movie-card">
  <div class="panel-heading">
    <h3>{{title}}</h3>
  </div>
  <div class="panel-body">
    <h4>{{year}}</h2>
    <img src="{{posterURL}}">
    <p>{{actors}}</p>
  </div>
  <div class="panel-footer">
    <button type="button" class="deleteButton btn btn-danger btn-sm">Delete This Movie</button>
    <h4>Movie Rating: {{rating}}</h4>
  </div>
</div>
