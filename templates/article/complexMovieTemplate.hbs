<div class="panel panel-primary col-md-4 movie-card">
    <div class="panel-heading">
        <h3>{{title}}</h3>
    </div>
<div class="panel-body">
<h4>{{year}}</h4>
<img class="movie-card-image" src="{{posterURL}}">
  </div>
  <div class="panel-footer">
  <button type="button" class="deleteButton">Delete This Movie</button>
  <h4>{{watched}}</h4>
  <h4>Movie Rating: {{rating}}</h4>
  </div>
</div>



